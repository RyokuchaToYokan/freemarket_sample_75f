
.header
  .inner
    .mainheader
      %h1.mainheader__logo
        %a{:href => "/"}
          = image_tag "material/logo/logo.png", class:"icon",size: "140x40"
      .mainheader__search
        .mainheader__search__form
          %form{"accept-charset" => "UTF-8", :action => "#", :method => "get"}
          %input{:name => "utf8", :type => "hidden", :value => "✓"}/
          %input.mainheader__search__form__line{:name => "key", :placeholder => "キーワードから探す"}/
          %form.button_to{:action => "/?type=submit", :method => "post"}
          %button{:type => "submit",class:"mainheader__search__form__submit__color"}
            .mainheader__search__form__submit__icon
              = image_tag "material/icon/icon-search 1.png", class:"mainheader__search__form__submit__icon",size: "20x20"
      -# %ul.resultLists
    .undernav
      %ul.underleft
        %li.lists__item
          %a#catbtn{:href => "#"} カテゴリー
        .lists__brand
          %h2
          ブランド
      .underright
        .underright__loginbtn
          %h2
            ログイン
        .underright__signupbtn
          %h2
            新規会員登録






